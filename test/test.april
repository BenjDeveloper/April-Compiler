fn div (a:int, b:int)
{
    if (b == 0) 
    { 
        return -1;
    }
    return (a/b);
}


println(div(1, 0));
println(div(1, 2));
println(div(1, 0));
println(div(1, 2));
println(div(5, 0));
println(div(5, 0));
println(div(5, 0));
println(div(5, 0));

