fn fibonacci(valor:int)
{
    if valor < 3 { if valor == 0 {return 0;} else {return 1;} }
    else { return fibonacci(valor-1)+fibonacci(valor-2); }
}

fn main()
{
    x := 12;
    result := fibonacci(x);
    println("result: %d", result);
}

main();

